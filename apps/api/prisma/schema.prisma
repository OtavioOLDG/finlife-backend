generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model entrada {
  id           Int            @id @default(autoincrement())
  nome         String
  entrada_info entrada_info[]
}

model entrada_categoria {
  id                       Int               @id @default(autoincrement())
  id_usuario_info_cadastro Int
  dthr_cadastro            DateTime          @db.Timestamp(6)
  nome                     String
  id_ativo                 Boolean
  id_grupo_financeiro      Int?
  id_patrimonial           Boolean
  grupo_financeiro         grupo_financeiro? @relation(fields: [id_grupo_financeiro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info             usuario_info      @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  entrada_info             entrada_info[]
}

model entrada_info {
  id                                                               Int                    @id @default(autoincrement())
  dthr_cadastro                                                    DateTime               @db.Timestamp(6)
  dthr_entrada                                                     DateTime?              @db.Timestamp(6)
  id_usuario_info_cadastro                                         Int
  id_info_ativo                                                    Boolean
  id_ativo                                                         Boolean
  id_entrada                                                       Int
  id_entrada_categoria                                             Int
  id_pagamento_entrada_tipo                                        Int
  id_usuario_info                                                  Int
  id_periodicidade                                                 Int
  valor                                                            Decimal                @db.Decimal(15, 2)
  id_patrimonio_info                                               Int?
  comprovante                                                      Int?                   @db.Oid
  entrada_categoria                                                entrada_categoria      @relation(fields: [id_entrada_categoria], references: [id], onDelete: NoAction, onUpdate: NoAction)
  entrada                                                          entrada                @relation(fields: [id_entrada], references: [id], onDelete: NoAction, onUpdate: NoAction)
  pagamento_entrada_tipo                                           pagamento_entrada_tipo @relation(fields: [id_pagamento_entrada_tipo], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info_entrada_info_id_usuario_info_cadastroTousuario_info usuario_info           @relation("entrada_info_id_usuario_info_cadastroTousuario_info", fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info_entrada_info_id_usuario_infoTousuario_info          usuario_info           @relation("entrada_info_id_usuario_infoTousuario_info", fields: [id_usuario_info], references: [id], onDelete: NoAction, onUpdate: NoAction)
  notificacao                                                      notificacao[]

  @@index([id_entrada], map: "idx_entrada_info_entrada")
  @@index([id_usuario_info], map: "idx_entrada_info_usuario")
}

model grupo_financeiro {
  id                       Int                        @id @default(autoincrement())
  id_ativo                 Boolean
  dthr_cadastro            DateTime                   @db.Timestamp(6)
  id_usuario_info_cadastro Int
  nome                     String
  entrada_categoria        entrada_categoria[]
  usuario_info             usuario_info               @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  grupo_financeiro_usuario grupo_financeiro_usuario[]
  pagamento_entrada_tipo   pagamento_entrada_tipo[]
  pagamento_saida_tipo     pagamento_saida_tipo[]
  saida_categoria          saida_categoria[]

  @@index([id_usuario_info_cadastro], map: "idx_grupo_financeiro_usuario")
}

model grupo_financeiro_cargo {
  id                       Int                        @id @default(autoincrement())
  nome                     String
  id_admin                 Boolean
  id_ativo                 Boolean
  id_usuario_info_cadastro Int
  dthr_cadastro            DateTime                   @db.Timestamp(6)
  usuario_info             usuario_info               @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  grupo_financeiro_usuario grupo_financeiro_usuario[]
}

model grupo_financeiro_usuario {
  id                                                                           Int                    @id @default(autoincrement())
  id_ativo                                                                     Boolean
  dthr_cadastro                                                                DateTime               @db.Timestamp(6)
  id_usuario_info_cadastro                                                     Int
  id_usuario_info                                                              Int
  id_grupo_financeiro_cargo                                                    Int
  id_grupo_financeiro                                                          Int
  grupo_financeiro_cargo                                                       grupo_financeiro_cargo @relation(fields: [id_grupo_financeiro_cargo], references: [id], onDelete: NoAction, onUpdate: NoAction)
  grupo_financeiro                                                             grupo_financeiro       @relation(fields: [id_grupo_financeiro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info_grupo_financeiro_usuario_id_usuario_info_cadastroTousuario_info usuario_info           @relation("grupo_financeiro_usuario_id_usuario_info_cadastroTousuario_info", fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info_grupo_financeiro_usuario_id_usuario_infoTousuario_info          usuario_info           @relation("grupo_financeiro_usuario_id_usuario_infoTousuario_info", fields: [id_usuario_info], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([id_usuario_info_cadastro], map: "idx_gf_usuario_cadastro")
  @@index([id_usuario_info], map: "idx_gf_usuario_info")
}

model notificacao {
  id              Int           @id @default(autoincrement())
  id_usuario_info Int
  dthr_cadastro   DateTime      @db.Timestamp(6)
  titulo          String
  descricao       String
  id_saida_info   Int?
  id_entrada_info Int?
  visto           Boolean
  entrada_info    entrada_info? @relation(fields: [id_entrada_info], references: [id], onDelete: NoAction, onUpdate: NoAction)
  saida_info      saida_info?   @relation(fields: [id_saida_info], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info    usuario_info  @relation(fields: [id_usuario_info], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model pagamento_entrada_tipo {
  id                       Int               @id @default(autoincrement())
  nome                     String
  dthr_cadastro            DateTime          @db.Timestamp(6)
  id_usuario_info_cadastro Int
  id_ativo                 Boolean
  id_grupo_financeiro      Int?
  entrada_info             entrada_info[]
  grupo_financeiro         grupo_financeiro? @relation(fields: [id_grupo_financeiro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info             usuario_info      @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model pagamento_saida_tipo {
  id                       Int               @id @default(autoincrement())
  nome                     String
  dthr_cadastro            DateTime          @db.Timestamp(6)
  id_usuario_info_cadastro Int
  id_ativo                 Boolean
  id_grupo_financeiro      Int?
  grupo_financeiro         grupo_financeiro? @relation(fields: [id_grupo_financeiro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info             usuario_info      @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  saida_info               saida_info[]
}

model patrimonio {
  id              Int               @id @default(autoincrement())
  nome            String
  valor_aquisicao Decimal           @db.Decimal(15, 2)
  patrimonio_info patrimonio_info[]
}

model patrimonio_info {
  id                       Int                      @id @default(autoincrement())
  dthr_cadastro            DateTime                 @db.Timestamp(6)
  id_ativo                 Boolean
  id_info_ativo            Boolean
  id_usuario_info_cadastro Int
  id_patrimonio            Int
  valor_mercado            Decimal                  @db.Decimal(15, 2)
  patrimonio               patrimonio               @relation(fields: [id_patrimonio], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info             usuario_info             @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  patrimonio_info_imagem   patrimonio_info_imagem[]

  @@index([id_patrimonio], map: "idx_patrimonio_info_patrimonio")
}

model patrimonio_info_imagem {
  id                       Int             @id @default(autoincrement())
  id_usuario_info_cadastro Int
  dthr_cadastro            DateTime        @db.Timestamp(6)
  imagem                   Int             @db.Oid
  id_patrimonio_info       Int
  patrimonio_info          patrimonio_info @relation(fields: [id_patrimonio_info], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info             usuario_info    @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model periodicidade {
  id                       Int          @id @default(autoincrement())
  id_usuario_info_cadastro Int
  id_ativo                 Boolean
  dthr_cadastro            DateTime     @db.Timestamp(6)
  nome                     String
  id_mensal                Boolean
  id_anual                 Boolean
  id_unico                 Boolean
  usuario_info             usuario_info @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model saida {
  id         Int          @id @default(autoincrement())
  nome       String
  saida_info saida_info[]
}

model saida_categoria {
  id                       Int               @id @default(autoincrement())
  id_usuario_info_cadastro Int
  dthr_cadastro            DateTime          @db.Timestamp(6)
  nome                     String
  id_ativo                 Boolean
  id_grupo_financeiro      Int?
  id_patrimonial           Boolean
  grupo_financeiro         grupo_financeiro? @relation(fields: [id_grupo_financeiro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info             usuario_info      @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  saida_info               saida_info[]
}

model saida_info {
  id                                                             Int                  @id @default(autoincrement())
  id_ativo                                                       Boolean
  dthr_cadastro                                                  DateTime             @db.Timestamp(6)
  dthr_saida                                                     DateTime?            @db.Timestamp(6)
  id_saida_categoria                                             Int
  id_pagamento_saida_tipo                                        Int
  id_usuario_info_cadastro                                       Int
  id_saida                                                       Int
  id_info_ativo                                                  Boolean
  id_saida_prioridade                                            Int
  id_usuario_info                                                Int
  id_periodicidade                                               Int
  valor                                                          Decimal              @db.Decimal(15, 2)
  id_patrimonio_info                                             Int?
  comprovante                                                    Int?                 @db.Oid
  notificacao                                                    notificacao[]
  pagamento_saida_tipo                                           pagamento_saida_tipo @relation(fields: [id_pagamento_saida_tipo], references: [id], onDelete: NoAction, onUpdate: NoAction)
  saida_categoria                                                saida_categoria      @relation(fields: [id_saida_categoria], references: [id], onDelete: NoAction, onUpdate: NoAction)
  saida                                                          saida                @relation(fields: [id_saida], references: [id], onDelete: NoAction, onUpdate: NoAction)
  saida_prioridade                                               saida_prioridade     @relation(fields: [id_saida_prioridade], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info_saida_info_id_usuario_info_cadastroTousuario_info usuario_info         @relation("saida_info_id_usuario_info_cadastroTousuario_info", fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
  usuario_info_saida_info_id_usuario_infoTousuario_info          usuario_info         @relation("saida_info_id_usuario_infoTousuario_info", fields: [id_usuario_info], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([id_saida], map: "idx_saida_info_saida")
  @@index([id_usuario_info], map: "idx_saida_info_usuario")
}

model saida_prioridade {
  id                       Int          @id @default(autoincrement())
  id_usuario_info_cadastro Int
  dthr_cadastro            DateTime     @db.Timestamp(6)
  id_ativo                 Boolean
  nivel                    Int
  nome                     String
  saida_info               saida_info[]
  usuario_info             usuario_info @relation(fields: [id_usuario_info_cadastro], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model usuario {
  id              Int            @id @default(autoincrement())
  nome            String
  sobrenome       String
  cpf             String         @unique
  dthr_nascimento DateTime       @db.Date
  usuario_info    usuario_info[]
}

model usuario_info {
  id                                                                                       Int                        @id @default(autoincrement())
  endereco                                                                                 String
  email                                                                                    String                     @unique
  senha                                                                                    String
  dthr_cadastro                                                                            DateTime                   @default(now()) @db.Timestamp(6)
  id_ativo                                                                                 Boolean
  id_info_ativo                                                                            Boolean
  id_usuario                                                                               Int
  entrada_categoria                                                                        entrada_categoria[]
  entrada_info_entrada_info_id_usuario_info_cadastroTousuario_info                         entrada_info[]             @relation("entrada_info_id_usuario_info_cadastroTousuario_info")
  entrada_info_entrada_info_id_usuario_infoTousuario_info                                  entrada_info[]             @relation("entrada_info_id_usuario_infoTousuario_info")
  grupo_financeiro                                                                         grupo_financeiro[]
  grupo_financeiro_cargo                                                                   grupo_financeiro_cargo[]
  grupo_financeiro_usuario_grupo_financeiro_usuario_id_usuario_info_cadastroTousuario_info grupo_financeiro_usuario[] @relation("grupo_financeiro_usuario_id_usuario_info_cadastroTousuario_info")
  grupo_financeiro_usuario_grupo_financeiro_usuario_id_usuario_infoTousuario_info          grupo_financeiro_usuario[] @relation("grupo_financeiro_usuario_id_usuario_infoTousuario_info")
  notificacao                                                                              notificacao[]
  pagamento_entrada_tipo                                                                   pagamento_entrada_tipo[]
  pagamento_saida_tipo                                                                     pagamento_saida_tipo[]
  patrimonio_info                                                                          patrimonio_info[]
  patrimonio_info_imagem                                                                   patrimonio_info_imagem[]
  periodicidade                                                                            periodicidade[]
  saida_categoria                                                                          saida_categoria[]
  saida_info_saida_info_id_usuario_info_cadastroTousuario_info                             saida_info[]               @relation("saida_info_id_usuario_info_cadastroTousuario_info")
  saida_info_saida_info_id_usuario_infoTousuario_info                                      saida_info[]               @relation("saida_info_id_usuario_infoTousuario_info")
  saida_prioridade                                                                         saida_prioridade[]
  usuario                                                                                  usuario                    @relation(fields: [id_usuario], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([id_usuario], map: "idx_usuario_info_usuario")
}
